{
  "name": "kronos-service-health-check",
  "version": "0.0.0-semantic-release",
  "description": "collects and reports system health status",
  "keywords": [
    "kronos-service"
  ],
  "main": "dist/module.js",
  "module": "src/ServiceHealthCheck.js",
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "build": "node_modules/rollup/bin/rollup --output=dist/module.js -c -- src/ServiceHealthCheck.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-health-check.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-health-check/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-health-check#readme",
  "dependencies": {
    "kronos-service": "^4.3.2",
    "model-attributes": "^3.0.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.39.2"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
